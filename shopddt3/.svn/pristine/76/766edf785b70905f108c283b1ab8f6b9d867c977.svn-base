.kucun_shoucang .shoucang{
	display:inline-block;
	float:right;
	height:35px;
	line-height:35px;
}
.kucun_shoucang .shoucang span{
	display:inline-block;
	position:relative;
	width:25px;
	height:25px;
	-webkit-transform:rotate(-45deg);
	-moz-transform:rotate(-45deg);
	-ms-transform:rotate(-45deg);
	-o-transform:rotate(-45deg);
	transform:rotate(-45deg);
	vertical-align:top;
}
.kucun_shoucang .shoucang span::before{
	content:"";
	position:absolute;
	display:block;
	width:10px;
	height:16px;
	left:0;
	top:2px;
	background:#d7d7d7;
	border-radius:5px 5px 0 0;
}
.kucun_shoucang .shoucang span::after{
	content:"";
	position:absolute;
	display:block;
	width:16px;
	height:10px;
	left:0;
	top:8px;
	background:#d7d7d7;
	border-radius:0 5px 5px 0;
}
.kucun_shoucang .shoucang .yishoucang::before{
	background:#f79c5c!important;
}
.kucun_shoucang .shoucang .yishoucang::after{
