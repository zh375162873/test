/*index.html*/
/**lbs*/
.home_lbs{
	position:absolute;
	top:4px;
	left:7px;
	z-index:999;
	font-size:12px;
}
.home_lbs a{
	background:rgba(255,255,255,0.8);
	padding:7px;
	border-radius:0 0 3px 3px;
}
.home_lbs a img{
	width:12px;
	height:15px;
}
.home_lbs a b{
	margin-left:5px;
}
button { font-family:"Microsoft YaHei";}
/**----end-----*/
.focus{max-width:100%;height:auto;margin:0 auto;position:relative;overflow:hidden;}
.focus .hd,.focus .nav_hd{width:100%;height:11px;position:absolute;z-index:1;bottom:10px;text-align:center;}
.focus .nav_hd{bottom:1px;}
.focus .hd ul,.focus .nav_hd ul{display:inline-block;height:5px;padding:3px 5px;font-size:0;vertical-align:top;}
/*.focus .hd ul li{display:inline-block;width:25px;height:4px;background:#8C8C8C;margin:0 5px;vertical-align:top;overflow:hidden;}*/
.focus .hd ul li,.focus .nav_hd ul li{display:inline-block;width:7px;height:7px;background:#8C8C8C;margin:0 5px;vertical-align:top;overflow:hidden;border-radius:50%;}
.focus .hd ul .on,.focus .nav_hd ul .on{background:#F60;}
.focus .bd,.focus .nav_bd{position:relative;z-index:0;text-align:center;}
.focus .bd li img{width:100%;background:url(../images/index/loading.gif) center center no-repeat;}
.focus .bd li a,.focus .nav_bd li a{-webkit-tap-highlight-color:rgba(0, 0, 0, 0);}
/*垂直分页*/
.focus .Vertical_hd{width:15px;max-height:100%;position:absolute;z-index:1;top:0;text-align:center;overflow:hidden;}
.focus .Vertical_hd ul{display:inline-block;height:100%;font-size:0;vertical-align:top;padding:0 5px;}
.focus .Vertical_hd ul li{display:inline-block;width:7px;height:7px;background:rgba(255,255,255,0.5);margin:3px 0;vertical-align:top;overflow:hidden;border-radius:50%;}
.focus .Vertical_hd ul .on{background:#f79c5c;}
