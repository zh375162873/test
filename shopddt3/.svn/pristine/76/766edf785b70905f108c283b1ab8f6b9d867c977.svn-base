/*order.html*/
/*order_goods*/
.order_goods{
	margin:10px 8px 0 8px;
	padding:10px 7px;
	background:#fff;
	border-radius:4px;
	box-shadow:1px 3px 5px #d8d7d7;
}
.order_goods a{
	display:block;
	overflow:hidden;
}
.order_goods a img{
	float:left;
	width:85px;
	height:85px;
	overflow:hidden;
}
.og_info{
	margin-left:100px;
	min-height:8px;
}
.og_info h3{
	height: 16px;
	line-height: 16px;
	max-width: 100%;
	font-size: 15px;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
	margin: 0;
	padding: 0;
	color:#353535;
}
.og_info p{
	position:relative;
	line-height: 16px;
	height: 32px;
	margin: 8px 0;
	overflow: hidden;
	font-size: 12px;
	padding-right:8px;
}
.og_info p::after{
	content:"";
	position:absolute;
	top:5px;
	right:3px;
	width: 7px;
	height: 7px;
	border-top: 1px solid #7d7d7d;
	border-right: 1px solid #7d7d7d;
	-webkit-transform: rotate(45deg);
	-moz-transform: rotate(45deg);
	-ms-transform: rotate(45deg);
	-o-transform: rotate(45deg);
	transform: rotate(45deg);
}
.og_info div{
	line-height: 20px;
	height: 20px;
	overflow: hidden;
	font-size: 19px;
	color:#d56564;
}
.og_tk{
	padding:10px 0 10px 15px;
	height:24px;
	border-bottom:1px solid #f3f3f3;
}
.og_tkLeft,.og_tkRight{
	float:left;
	width:50%;
	height:24px;
	vertical-align:top;
	color:#f6882d;
}
.og_tkIcon{
	display:inline-block;
	position:relative;
	margin-right:5px;
	width:18px;
	height:18px;
	overflow:hidden;
	border-radius:20px;
}
.og_tkIcon::before{
	content: "";
	display: block;
	position: absolute;
	width: 7px;
	height: 11px;
	top: -1px;
	left: 4px;
	-webkit-transform: rotate(32deg);
	-moz-transform: rotate(32deg);
	-ms-transform: rotate(32deg);
	-o-transform: rotate(32deg);
	transform: rotate(32deg);
}
.iconYes{
	border:2px solid #f6882d;
}
.iconYes::before{
	border-bottom: 2px solid #f6882d;
	border-right: 2px solid #f6882d;
}
.iconNo{
	border:2px solid #7d7d7d;
}
.iconNo::before{
	border-bottom: 2px solid #7d7d7d;
	border-right: 2px solid #7d7d7d;
}
.og_desc section{
	margin-top:10px;
	border-bottom:1px solid #f3f3f3;
}
.og_desc section:last-child{
	margin-top:15px;
	border-bottom:none;
}
.og_desc section div{
	display:inline-block;
	font-size:14px;
	vertical-align:middle;
}
.og_desc section div span{
	color:#353535;
}
.og_desc .dhm{
	position: relative;
	padding-bottom:10px;
	width:100%;
}
.og_desc .dhm span{
	font-size:18px;
}
.og_desc .dhm .order_status{
	position: absolute;
    padding-top: 3px;
    font-weight: normal;
    right: 0;
    top: 0;
}
.og_desc .btn_div{
	width:100%;
	padding-bottom:5px;
	text-align:right;
}
.og_desc .btn_div a.zs{
	margin-left:45px;
}
.og_desc .btn_div a{
	display:inline-block;
	width:100px;
	height: 20px;
	color: #fff;
	font-size:14px;
	background:#f79c5c;
	line-height: 20px;
	text-align: center;
	vertical-align:middle;
	-webkit-border-radius:11px;
	-moz-border-radius:11px;
	border-radius:4px;
}
.order_dhm{
	margin: 10px 5px;
	padding: 10px 7px;
	background: #fff;
	border-radius: 4px;
	box-shadow: 1px 3px 5px #d8d7d7;
}
.order_dhm h3{
	margin: 0;
	padding: 0 0 5px 0;
	font-size: 15px;
	color: #353535;
	border-bottom: 1px solid #f3f3f3;
}

.order_info{
	margin:10px 5px 50px 5px;
	padding:10px 7px;
	background:#fff;
	border-radius:4px;
	box-shadow:1px 3px 5px #d8d7d7;
}
.order_info h3{
	margin:0;
	padding:0;
	font-size: 15px;
	color:#353535;
}
.order_info table{
	max-width:100%;
	margin-top:6px;
	font-size:14px;
	border:none;
}
.order_info table tr td{
	border:none;
	text-align:left;
	padding-bottom:5px;
}
.order_info table tr td:first-child{
	text-align:right;
}
.order_info table tr td:last-child{
	padding-left:15px;
}

