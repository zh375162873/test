@charset "utf-8";
/* CSS Document */
    /*前台展示----------------------------------------------------------------------*/
    /*公用----------------------------*/
    body{
        margin:0;
        padding:0;
        font-size:100%;
        margin:0 auto;
        color:#555;
        font-family:arial,sans-serif;font:16px/1.5 "Microsoft YaHei",\5fae\8f6f\96c5\9ed1,arial,\5b8b\4f53;
        outline:0;
        background-color:#F3F3F3;
    }
    ul{
        list-style:none;
        margin:0;
        padding:0;
    }
    a{
        text-decoration:none;
        color:#555;
    }
    .ddt_block{
        overflow:hidden;
    }
    img{
        vertical-align:top;
        border:none;
        outline:none;
    }
    b { font-family:arial,sans-serif;font:16px/1.5 "Microsoft YaHei",\5fae\8f6f\96c5\9ed1,arial,\5b8b\4f53;}
    /**font-color*/
    .colorC3{color:#e1544f;}
    .color54{color:#545454;}
    .colorDb{color:#db5e3b;}
    .colorE7{color:#e7c151;}
    .colorBb{color:#bbe196;}
    .colorC6{color:#c680b8;}
    .colorAe{color:#aeaeae;}
    .color35{color:#313131;}
    .color7d{color:#7d7d7d;}
    .colorD5{color:#d14042;}
    /*结束----公用------------------------*/
    
    
     /*顶部导航*/
    .home_headerNav{
        min-heifht:40px;
        background:#fff;
        overflow:hidden;
    }
    .home_headerNav>div{
        -moz-box-sizing:border-box;
        -webkit-box-sizing:border-box;
        box-sizing:border-box;
        text-align:center;
        line-height:40px;
    }
    .home_headerNav .header_left{
        float:left;
        vertical-align:middle;
    }
    .home_headerNav .header_right{
        float:right;
    }
    .home_headerNav .header_left,.home_headerNav .header_right{
        width:80px;
        height:40px;
    }
    .home_headerNav .header_left a,.home_headerNav .header_right a{
        display:inline-block;
        height:40px;
        font-size:13px;
    }
    .home_headerNav .header_left a{
        -webkit-box-sizing:border-box;
        -moz-box-sizing:border-box;
        box-sizing:border-box;
        padding-top:5px;
    }
    .home_headerNav .header_left a img{
        height:19px;
        width:15px;
        vertical-align:initial;
    }
    .home_headerNav .header_left a b{
        display:inline-block;
        padding-left:3px;
        font-size:13px;
        max-width:62px;
        white-space: nowrap;
        overflow: hidden;
        text-overflow: ellipsis;
    }
    .home_headerNav .header_search{
        position:relative;
        margin:0 80px;
        height:40px;
    }
    .home_headerNav .header_search a input{
        border:none;
        outline:none;
        -webkit-box-sizing:border-box;
        -moz-box-sizing:border-box;
        box-sizing:border-box;
        padding:0 5px 0 40px;
        width:100%;
        height:38px;
        border-radius:4px;
        background:#f3f3f3;
        vertical-align:top;
        margin-top:1px;
    }
    .home_headerNav .header_search a span{
        display:block;
        width:32px;
        height:33px;
        position:absolute;
        top:3px;
        left:3px;
        background:url(./images/index/nav_search_0.png) center;
        background-size:100% 100%;
        -webkit-box-sizing:border-box;
        -moz-box-sizing:border-box;
        box-sizing:border-box;
    }

    /*search_nav*/
    .search_nav{
        height:34px;
        background:#f45749;
        padding-top:6px;
    }
    .search_nav .header_search{
        position:relative;
        margin:0 15px 6px 15px;
    }
    .search_nav .header_search input{
        border:none;
        outline:none;
        -webkit-box-sizing:border-box;
        -moz-box-sizing:border-box;
        box-sizing:border-box;
        padding:0 5px 0 45px;
        width:100%;
        height:26px;
        border-radius:4px;
        background:#f3f3f3;
        vertical-align:top;
        margin-top:1px;
    }
    .search_nav .header_search a{
        display:block;
        width:40px;
        height:28px;
        position:absolute;
        top:0;
        left:0;
    }
    .search_nav .header_search a span{
        display:block;
        width:28px;
        height:28px;
        margin:0 5px;
        background:url(./images/index/nav_search_0.png) center;
        background-size:100% 100%;
    }
    .home_headerNav ul{
        list-style:none;
        margin:0;
        padding:0;
        max-height:150px;
        overflow-y:auto;
    }
    .search_nav ul li{
        padding:5px 10px;
        border-bottom:1px solid #f3f3f3;
    }
    .search_nav ul li:first-child{
        margin-top:10px;
    }
    .search_nav ul li:last-child{
        margin-bottom:10px;
        border-bottom:none;
    }
    

    /**--------顶部定位-------------------*/
    .home_lbs{
        position:absolute;
        top:0;
        left:0.625em;
        z-index:999;
        font-size:12px;
    }
    .home_lbs a{
        display:inline-block;
        background:rgba(255,255,255,0.8);
        padding:0.625em;
        border-radius:0 0 0.8em 0.8em;
    }
    .home_lbs a img{
        width:14px;
    }
    .home_lbs a b{
        display:none;
        margin-left:5px;
        font-size:14px;
    }
    /**结束---------顶部定位------------------*/



    /**--------商品列表区域-------------------*/
    .home_list{
        margin:10px 8px 0 8px;
        background:#fff;
        border-radius:5px;
        overflow:hidden;
        box-shadow:1px 3px 5px #d8d7d7;
    }
    .home_list section{
        overflow:hidden;
        border-bottom:1px solid #f3f3f3;
    }
    .list_nav_title{
        display:inline-block;
        padding:9px 6px;
        margin-left: 3px;
    }
    .list_nav_img{
        float:right;
        height:43px;
        width:auto;
    }
    .goods_content{
        display: block;
        padding: 12px 6px 12px 0;
        margin-left: 9px;
        text-decoration: none;
    }
    .goodImg{
        float: left;
        position: relative;
        width: 85px;
        overflow: hidden;
        /*background:url(./res/bg.png) no-repeat;*/
        background-size: 100% 100%;
    }
    .goodImg img{
        display: block;
        width: 85px;
        height: 85px;
    }
    .goodInfo{
        position:relative;
        margin-left:100px;
    }
    .title_line{
        line-height: 16px;
        height: 16px;
        overflow: hidden;
        margin-top:4px;
    }
    .title_line .title{
        float:left;
        height:16px;
        line-height:16px;
        max-width:100%;
    }
    .title_line .title .item_name{
        max-width: 100%;
        font-size: 16px;
        white-space: nowrap;
        overflow: hidden;
        text-overflow: ellipsis;
        margin:0;
        padding:0;
    }
    .title_line .wz{
        display:none;
        float: right;
        font-size: 12px;
    }
    .goodInfo .desc{
        line-height: 16px;
        height: 32px;
        margin: 8px 0;
        overflow: hidden;
        display: -webkit-box;
        -webkit-line-clamp: 2;
        -webkit-box-orient: vertical;
        font-size: 12px;
    }
    .price_line{
        line-height: 16px;
        height: 16px;
        font-size: 0;
        overflow: hidden;
    }
    .price_line ins{
        margin-right: 5px;
        font-size: 16px;
        font-weight: 700;
        text-decoration: none;
    }
    .price_line del{
        margin-right: 5px;
        font-size: 12px;
    }
    .price_line span{
        float: right;
        margin-left: 16px;
        font-size: 12px;
    }
    /**结束--------商品列表区域-------------------*/
    /**------------底部按钮区域--------------------------*/
    .home_footer{
        position:fixed;
        height:55px;
        z-index: 999;
        left:0;
        bottom:0;
        width:100%;
        text-align:center;
    }
    .home_footer ul{
        width:100%;
        height:55px;
    }
    .home_footer ul li{
        margin-top:15px;
        float: left;
        display:block;
        width:30%;
        height:40px;
        background: rgba(255,255,255,0.7);
        text-align:center;
        line-height:40px;
        font-size:16px;
    }
    .home_footer ul li a{
        width:100%;
        height:100%;
        display:block;
    }
    .home_footer ul li:first-child,.home_footer ul li:last-child{
        width:35%;
    }
    .home_footer ul li:first-child img,.home_footer ul li:last-child img{
        width:25px;
        height:25px;
        margin:7px 5px;
    }
    .home_footer_hd{
        width:55px;
        height:55px;
        margin-top:-15px;
    }
    .home_footer_num{
        position:relative;
        display:inline-block;
        min-width:8px;
        height:19px;
        line-height:20px;
        font-size:12px;
        top:-10px;
        color:red;
    }
    /*未登录动画*/
    .loginOut{
        position:relative;
        -webkit-animation: myscale 1s infinite alternate;
        -o-animation: myscale 1s infinite alternate;
        animation: myscale 1s infinite alternate;
    }
    @-webkit-keyframes myscale {
        from {-webkit-transform: scale(1,1);-o-transform: scale(1,1);transform: scale(1,1);}
        to {-webkit-transform: scale(1.2, 1.2);-o-transform: scale(1.2, 1.2);transform: scale(1.2, 1.2);  }
    }
    @-o-keyframes myscale {
        from {-webkit-transform: scale(1,1);-o-transform: scale(1,1);transform: scale(1,1);}
        to {-webkit-transform: scale(1.2, 1.2);-o-transform: scale(1.2, 1.2);transform: scale(1.2, 1.2);  }
    }
    @keyframes myscale {
        from {-webkit-transform: scale(1,1);-o-transform: scale(1,1);transform: scale(1,1);}
        to {-webkit-transform: scale(1.2, 1.2);-o-transform: scale(1.2, 1.2);transform: scale(1.2, 1.2);  }
    }
    /*购物车动画*/
    .shopCartAnimate{
        -webkit-animation-name: bounce;
        animation-name: bounce;
        -webkit-transform-origin: center bottom;
        transform-origin: center bottom;
        -webkit-animation-duration: 4s;
        animation-duration: 4s;
        -webkit-animation-fill-mode: both;
        animation-fill-mode: both;
        -webkit-animation-iteration-count: infinite;
        animation-iteration-count: infinite;
    }
    @-webkit-keyframes bounce {
        from, 4%, 10.6%, 16%, 20%,to {
            -webkit-animation-timing-function: cubic-bezier(0.215, 0.610, 0.355, 1.000);
            animation-timing-function: cubic-bezier(0.215, 0.610, 0.355, 1.000);
            -webkit-transform: translate3d(0,0,0);
            transform: translate3d(0,0,0);
        }
        8%, 8.3% {
            -webkit-animation-timing-function: cubic-bezier(0.755, 0.050, 0.855, 0.060);
            animation-timing-function: cubic-bezier(0.755, 0.050, 0.855, 0.060);
            -webkit-transform: translate3d(0, -10px, 0);
            transform: translate3d(0, -10px, 0);
        }
        14% {
            -webkit-animation-timing-function: cubic-bezier(0.755, 0.050, 0.855, 0.060);
            animation-timing-function: cubic-bezier(0.755, 0.050, 0.855, 0.060);
            -webkit-transform: translate3d(0, -7px, 0);
            transform: translate3d(0, -7px, 0);
        }
        18% {
            -webkit-transform: translate3d(0,-1px,0);
            transform: translate3d(0,-1px,0);
        }
    }
    @keyframes bounce {
        from, 4%, 10.6%, 16%, 20%,to {
            -webkit-animation-timing-function: cubic-bezier(0.215, 0.610, 0.355, 1.000);
            animation-timing-function: cubic-bezier(0.215, 0.610, 0.355, 1.000);
            -webkit-transform: translate3d(0,0,0);
            transform: translate3d(0,0,0);
        }
        8%, 8.3% {
            -webkit-animation-timing-function: cubic-bezier(0.755, 0.050, 0.855, 0.060);
            animation-timing-function: cubic-bezier(0.755, 0.050, 0.855, 0.060);
            -webkit-transform: translate3d(0, -10px, 0);
            transform: translate3d(0, -10px, 0);
        }
        14% {
            -webkit-animation-timing-function: cubic-bezier(0.755, 0.050, 0.855, 0.060);
            animation-timing-function: cubic-bezier(0.755, 0.050, 0.855, 0.060);
            -webkit-transform: translate3d(0, -7px, 0);
            transform: translate3d(0, -7px, 0);
        }
        18% {
            -webkit-transform: translate3d(0,-1px,0);
            transform: translate3d(0,-1px,0);
        }
    }
    /**结束------------底部按钮区域--------------------------*/
    .home_list_end{
        margin-bottom:50px;
    }
    /*--展示区结束---------------------------------------------------------------------------*/  
