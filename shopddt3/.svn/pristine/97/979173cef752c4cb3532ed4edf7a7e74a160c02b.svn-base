ul{ list-style-type:none; margin:0px; padding:0px;}
.btn{ font-weight:normal; text-shadow:none;}
.btn:focus{ background:#fd6759; color:#fff; outline:none; text-shadow:none;}
/*通用CSS集*/
/*所有的4个边框统一色为d6e1ee*/
.border_d6e1ee{ border:1px solid #d6e1ee;}

/*所有的bottom边框统一为1色为d6e1ee*/
