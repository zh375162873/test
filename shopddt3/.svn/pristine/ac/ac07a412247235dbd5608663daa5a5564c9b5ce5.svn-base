@charset "utf-8";
/* CSS Document */
 body{
            margin:0;
            padding:0;
            font-size:100%;
            margin:0 auto;
            color:#555;
            font-family:arial,sans-serif;font:16px/1.5 "Microsoft YaHei",\5fae\8f6f\96c5\9ed1,arial,\5b8b\4f53;
			outline:0;
			background-color:#F3F3F3;
        }
        ul{
            list-style:none;
            margin:0;
            padding:0;
        }
        a{
            text-decoration:none;
            color:#555;
        }
        .ddt_block{
            overflow:hidden;
        }
        img{
            vertical-align:top;
        }
b { font-family:arial,sans-serif;font:16px/1.5 "Microsoft YaHei",\5fae\8f6f\96c5\9ed1,arial,\5b8b\4f53;}
        /**lbs*/
        .home_lbs{
            position:absolute;
            top:0.3125em;
            left:0.625em;
            z-index:999;
            font-size:12px;
        }
        .home_lbs a{
            background:rgba(255,255,255,0.8);
            padding:0.625em;
            border-radius:0 0 0.8em 0.8em;
        }
        .home_lbs a img{
            width:18%;
        }
        .home_lbs a b{
            margin-left:5px;
			font-size:14px;
        }
        /**----end-----*/
        .focus{max-width:100%;height:auto;margin:0 auto;position:relative;overflow:hidden;}
        .focus .hd,.focus .nav_hd{width:100%;height:11px;position:absolute;z-index:1;bottom:10px;text-align:center;}
        .focus .nav_hd{bottom:0.1px;}
        .focus .hd ul,.focus .nav_hd ul{display:inline-block;height:5px;padding:3px 5px;font-size:0;vertical-align:top;}
        /*.focus .hd ul li{display:inline-block;width:25px;height:4px;background:#8C8C8C;margin:0 5px;vertical-align:top;overflow:hidden;}*/
        .focus .hd ul li,.focus .nav_hd ul li{display:inline-block;width:7px;height:7px;background:rgba(255,255,255,0.5);margin:0 5px;vertical-align:top;overflow:hidden;border-radius:50%;}
        .focus .hd ul .on,.focus .nav_hd ul .on{background:#f79c5c;}
        .focus .bd,.focus .nav_bd{position:relative;z-index:0;text-align:center;}
        .focus .bd li img{width:100%;background:url(./images/index/loading.gif) center center no-repeat;}
        .focus .bd li a,.focus .nav_bd li a{-webkit-tap-highlight-color:rgba(0, 0, 0, 0);}
        /**分类*/
        .home_sort{
            padding:0;
            margin:0;
            min-height:170px;
            overflow:hidden;
        }
        .home_sort li a{
            display:block;
            width:100%;
            overflow:hidden;
        }
        .home_sort li a img{
            margin:7px 0 0 0;
            width:60%;
        }
        .home_sort li a p{
            margin:0;
            padding:0;
            text-align:center;
            font-size:12px;
        }
        .home_sort li{
            float: left;
            display:block;
            width:25%;
            min-height:80px;
            text-align:center;
            overflow:hidden;
        }

        /**font-color*/
        .colorC3{color:#e1544f;}
        .color54{color:#545454;}
        .colorDb{color:#db5e3b;}
        .colorE7{color:#e7c151;}
        .colorBb{color:#bbe196;}
        .colorC6{color:#c680b8;}
        .colorAe{color:#aeaeae;}
        .color35{color:#313131;}
        .color7d{color:#7d7d7d;}
        .colorD5{color:#d14042;}

        /**活动*/
        .home_nav_huodong{
            margin:8px 8px 0 8px;
            background:#fff;
            border-radius:5px;
            overflow:hidden;
            box-shadow:1px 3px 5px #d8d7d7;
        }
        .home_nav_huodong ul li{
            float:left;
            display:block;
            width:50%;
            -webkit-box-sizing:border-box;
            box-sizing:border-box;
        }
        .home_nav_huodong ul li a{
            display:block;
            overflow:hidden;
        }
        .borderBm{
            border-bottom:1px solid #f3f3f3;
        }
        .borderRt{
            border-right:1px solid #f3f3f3;
        }
        .huodong_title{
            float:left;
            margin:10px 0 5px 11px;
        }
        .huodong_title span{
            display:block;
            font-size:16px;
        }
        .huodong_title small{
            color:#aeaeae;
            font-size:12px;
        }
        .huodong_img{
            float:right;
            height:49px;
            width:auto;
			max-width: 55px;
			margin-top: 5px;
        }

        /**goods*/
        .home_list{
            margin:10px 8px 0 8px;
            background:#fff;
            border-radius:5px;
        }
        .home_list section{
            overflow:hidden;
            border-bottom:1px solid #f3f3f3;
        }
        .list_nav_title{
            display:inline-block;
            padding:9px 6px;
		    margin-left: 3px;
        }
        .list_nav_img{
            float:right;
            height:43px;
            width:auto;
        }
        .goods_content{
            display: block;
            padding: 12px 6px 12px 0;
            margin-left: 9px;
            text-decoration: none;
        }
        .goodImg{
            float: left;
            position: relative;
            width: 85px;
            overflow: hidden;
            background:url(./res/bg.png) no-repeat;
            background-size: 100% 100%;
        }
        .goodImg img{
            display: block;
            width: 85px;
            height: 85px;
        }
        .goodInfo{
            position:relative;
            margin-left:100px;
        }
        .title_line{
            line-height: 16px;
            height: 16px;
            overflow: hidden;
            margin-top:4px;
        }
        .title_line .title{
            float:left;
            height:16px;
            line-height:16px;
            max-width:100%;
        }
        .title_line .title .item_name{
            max-width: 100%;
            font-size: 16px;
            white-space: nowrap;
            overflow: hidden;
            text-overflow: ellipsis;
            margin:0;
            padding:0;
        }
        .title_line .wz{
            display:none;
            float: right;
            font-size: 12px;
        }
        .goodInfo .desc{
            line-height: 16px;
            height: 32px;
            margin: 8px 0;
            overflow: hidden;
            display: -webkit-box;
            -webkit-line-clamp: 2;
            -webkit-box-orient: vertical;
            font-size: 12px;
        }
        .price_line{
            line-height: 16px;
            height: 16px;
            font-size: 0;
            overflow: hidden;
        }
        .price_line ins{
            margin-right: 5px;
            font-size: 16px;
            font-weight: 700;
            text-decoration: none;
        }
        .price_line ins::before{
            content:"¥"
            display:none;
        }
        .price_line ins::after{
            content:"元"
        }
        .price_line del{
            margin-right: 5px;
            font-size: 12px;
        }
        .price_line del::after{
            content:"元"
        }
        .price_line span{
            float: right;
            margin-left: 16px;
            font-size: 12px;
        }
        .price_line span::before{
            content:"剩余"
        }
        .price_line span::after{
            content:"份"
        }
        /**end-goods-----------*/
        /**footer-----------*/
        .home_footer{
            position:fixed;
            height:55px;
            z-index: 999;
            left:0;
            bottom:0;
            overflow:hidden;
            width:100%;
            text-align:center;
        }
        .home_footer ul{
            width:100%;
            height:55px;
        }
        .home_footer ul li{
            margin-top:15px;
            float: left;
            display:block;
            width:30%;
            height:40px;
            background: rgba(255,255,255,0.7);
            text-align:center;
            line-height:40px;
            font-size:16px;
        }
        .home_footer ul li a{
            width:100%;
            height:100%;
            display:block;
        }
        .home_footer ul li:first-child,.home_footer ul li:last-child{
            width:35%;
        }
        .home_footer ul li:first-child img,.home_footer ul li:last-child img{
            width:25px;
            height:25px;
            margin:7px 5px;
        }
        .home_footer_hd{
            width:55px;
            height:55px;
            margin-top:-15px;
        }
		/*未登录动画*/
		.loginOut{
            position:relative;
            -webkit-animation: myscale 1s infinite alternate;
            -o-animation: myscale 1s infinite alternate;
            animation: myscale 1s infinite alternate;
        }
        @-webkit-keyframes myscale {
            from {-webkit-transform: scale(1,1);-o-transform: scale(1,1);transform: scale(1,1);}
            to {-webkit-transform: scale(1.2, 1.2);-o-transform: scale(1.2, 1.2);transform: scale(1.2, 1.2);  }
        }
        @-o-keyframes myscale {
            from {-webkit-transform: scale(1,1);-o-transform: scale(1,1);transform: scale(1,1);}
            to {-webkit-transform: scale(1.2, 1.2);-o-transform: scale(1.2, 1.2);transform: scale(1.2, 1.2);  }
        }
        @keyframes myscale {
            from {-webkit-transform: scale(1,1);-o-transform: scale(1,1);transform: scale(1,1);}
            to {-webkit-transform: scale(1.2, 1.2);-o-transform: scale(1.2, 1.2);transform: scale(1.2, 1.2);  }
        }
        /**end-footer-----------*/
        .home_list_end{
            margin-bottom:40px;
        }
        .home_list_end img{
            width:auto;
            height:50px;
        }