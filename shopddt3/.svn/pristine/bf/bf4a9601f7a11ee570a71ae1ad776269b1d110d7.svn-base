/*兑换码列表dhm_list.html*/
.dhm_div{
    margin-bottom:60px;
}
.dhm_div .dhm_list_item{
    border-bottom:1px solid #f3f3f3;
}
.dhm_div .dhm_list_item:last-child{
    border-bottom:none;
}
.dhm_list_item{
    position:relative;
    min-height:90px;
    margin-top:10px;
    padding-bottom:10px;
}
.dhm_list_item a{
    display:block;
}
.dhm_list_item .l{
    position:absolute;
    width:80px;
    height:80px;
    top:0;
    left:0;
    text-align:center;
}
.dhm_list_item .l img{
    max-width:100%;
    max-height:100%;
    margin-top:18px;

}
.dhm_list_item .r{
    margin-left:90px;
    min-height:90px;
}
.dhm_list_item .r h3{
    margin:0;
}
.dhm_list_item .r p{
    margin:10px 10px 10px 0;
    font-size:18px;
    color:#f79c5c;
    position:relative;
}
.dhm_list_item .r p::after{
  content: "";
  display: block;
  position: absolute;
  top: 5px;
  right: -8px;
  font-size: 18px;
  width: 8px;
  height: 14px;
  background: url(../images/btn_more.png) no-repeat;
  background-size: 100% 100%;
}